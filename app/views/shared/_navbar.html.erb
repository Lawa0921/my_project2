        </li>
        <li class="nav-item active">
          <a class="nav-link" href="#">線上訂房</a>
        </li>
      </ul>
      <% if session[:login_token]%>
        <div class="nav-link">
        <%= link_to "您好 #{session[:login_token]}", url: user_path %>
        </div>
        <%= link_to "登出", login_path, method:'delete',class:'nav-link' %>
      <% else %>
        <%= link_to "註冊", new_user_path, class:'nav-link' %>
        <%= link_to "登入", login_path, class:'nav-link'%>
      <% end %>
      <form class="form-inline my-2 my-lg-0">
        <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
      </form>
